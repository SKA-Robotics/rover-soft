robot_hardware:
  - wheels_left
  - wheels_right
wheels_left:
  type: roboclaw/RoboclawHWInterface
  motor1: left_back_wheel_joint
  motor2: left_front_wheel_joint
wheels_right:
  type: roboclaw/RoboclawHWInterface
  motor1: right_back_wheel_joint
  motor2: right_front_wheel_joint
roboclaw_driver:
  baudrate: 115200
  "129": 
    communication_timeout: 0.3
    multiunit_mode: false
    relay_mode: false
    swap_encoders: false
    swap_buttons: false
    motor2:
      command_timeout: 800
      ticks_per_revolution: 172000
      torque_constant: 1.2986
      absolute_encoder: false
      reverse_motor: false
      reverse_encoder: true
      max_current: 4
      position_pid:
        MaxPos: 75000
        MinPos: -75000
      velocity_pid:
        P: 0.5009796142578125
        I: 0.03905029296875
        D: 0.0
        QPPS: 2.794556255809525
      # velocity_pid:
      #   P: 0.3891754150390625
      #   I: 0.0250396728515625
      #   D: 0.0
      #   QPPS: 3.6507133142070134
    motor1:
      command_timeout: 800
      ticks_per_revolution: 172000
      torque_constant: 1.2986
      absolute_encoder: false
      reverse_motor: true
      reverse_encoder: true
      max_current: 25
